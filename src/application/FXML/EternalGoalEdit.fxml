<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="400.0" stylesheets="@../application.css" xmlns="http://javafx.com/javafx/8." xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EternalGoalEditController">
   <children>
      <Label layoutX="29.0" layoutY="54.0" text="目標時間">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="time" layoutX="155.0" layoutY="55.0" prefHeight="25.0" prefWidth="59.0" />
      <Button fx:id="goalAddBtn" layoutX="214.0" layoutY="106.0" mnemonicParsing="false" onAction="#onGoalAddBtn" styleClass="orangeBtn" text="更 新">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Button>
      <Button fx:id="cancelBtn" layoutX="299.0" layoutY="106.0" mnemonicParsing="false" onAction="#onCancelBtn" styleClass="redBtn" text="キャンセル">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
      </Button>
      <Label layoutX="214.0" layoutY="54.0" text="時間">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="minute" layoutX="262.0" layoutY="55.0" prefHeight="25.0" prefWidth="59.0" />
      <Label layoutX="321.0" layoutY="54.0" text="分">
         <font>
            <Font name="Arial" size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
